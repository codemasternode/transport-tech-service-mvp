{
  "name": "transport-tech-service-backend",
  "version": "1.0.0",
  "description": "Jest to wersja backendowa aplikacji mającej służyć do obliczania kosztów spedycji",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/index.js",
    "start": "mode=PRODUCTION babel-node ./src/index.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require @babel/register --recursive './test/**/*.spec.js'",
    "debug": "node --inspect-brk ./dist/index.js",
    "build": "babel src --out-dir ./dist --source-maps",
    "test-watch": "NODE_ENV=test nodemon --watch test --exec './node_modules/.bin/mocha --require @babel/register --recursive './test/**/*.spec.js''"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "mongodb"
  ],
  "author": "Teach Tech Service Marcin Warzybok",
  "license": "ISC",
  "dependencies": {
    "@google/maps": "^1.0.2",
    "aws-sdk": "^2.606.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "csurf": "^1.10.0",
    "dotenv": "^8.0.0",
    "email-templates": "^2.7.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-validator": "^6.3.1",
    "google-distance-matrix": "^1.1.1",
    "googleapis": "^41.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.4",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.3.0",
    "nodemailer-express-handlebars": "^3.1.0",
    "pug": "^2.0.4",
    "redis": "^3.1.1",
    "saslprep": "^1.0.3",
    "stripe": "^8.15.0",
    "uniqid": "^5.0.3",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1"
  }
